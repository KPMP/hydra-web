    "@fortawesome/fontawesome-free": "6.0.0",
    "@fortawesome/fontawesome-svg-core": "1.3.0",
    "@fortawesome/free-brands-svg-icons": "6.0.0",
    "@fortawesome/free-regular-svg-icons": "6.0.0",
    "@fortawesome/free-solid-svg-icons": "6.0.0",
    "@fortawesome/react-fontawesome": "0.1.7",
    "axios": "0.24.0",
    "bootstrap-css-only": "4.4.1",
    "history": "5.1.0",
    "immutability-helper": "3.1.1",
    "isomorphic-unfetch": "3.1.0",
    "kpmp-common-components": "1.1.13",
    "kpmp-common-styles": "1.0.12",
    "node-sass-chokidar": "1.5.0",
    "npm-run-all": "4.1.5",
    "pretty-bytes": "^6.0.0",
    "react": "17.0.2",
    "react-dnd": "15.1.1",
    "react-dnd-html5-backend": "15.1.2",
    "react-dom": "17.0.2",
    "react-ga": "3.3.0",
    "react-ga4": "1.4.0",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.1",
    "react-scripts": "4.0.3",
    "reactstrap": "8.9.0",
    "redux": "4.1.1",
    "redux-thunk": "2.3.0",
    "vitessce": "1.1.21"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js --max_old_space_size=5120",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-js": "craco --max_old_space_size=5120 start",
    "build-js": "craco --max_old_space_size=5120 build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "analyze": "REACT_APP_RUN_ANALYZER=true npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "6.3.0",
    "craco-alias": "3.0.1",
    "graphql": "15.7.2",
    "webpack-bundle-analyzer": "4.5.0"
  }
}
